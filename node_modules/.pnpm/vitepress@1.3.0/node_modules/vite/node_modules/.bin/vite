#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/docs/awei-docs/node_modules/.pnpm/vite@5.3.3/node_modules/vite/bin/node_modules:/mnt/d/docs/awei-docs/node_modules/.pnpm/vite@5.3.3/node_modules/vite/node_modules:/mnt/d/docs/awei-docs/node_modules/.pnpm/vite@5.3.3/node_modules:/mnt/d/docs/awei-docs/node_modules/.pnpm/node_modules:/mnt/d/docs/awei-docs/node_modules:/mnt/d/docs/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/docs/awei-docs/node_modules/.pnpm/vite@5.3.3/node_modules/vite/bin/node_modules:/mnt/d/docs/awei-docs/node_modules/.pnpm/vite@5.3.3/node_modules/vite/node_modules:/mnt/d/docs/awei-docs/node_modules/.pnpm/vite@5.3.3/node_modules:/mnt/d/docs/awei-docs/node_modules/.pnpm/node_modules:/mnt/d/docs/awei-docs/node_modules:/mnt/d/docs/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
